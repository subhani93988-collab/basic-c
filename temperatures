#include <stdio.h>
int main() {
       int choice;
    float temp;
    scanf("%d %f", &choice, &temp);
    if(choice == 1)
        printf("Celsius %.2f", (temp - 32) * 5/9);
    else if(choice == 2)
        printf("Fahrenheit %.2f", (temp * 9/5) + 32);
    else
        printf("Invalid Choice");
    return 0;
}
