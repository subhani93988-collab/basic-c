#include <stdio.h>
int main() {
     int type, trips, amount = 0;
    scanf("%d %d", &type, &trips);
    switch(type) {
        case 1:
            if(trips == 1)
                amount = 100;
            else
                amount = 800;
            break;

        case 2:
            amount = trips * 240;
            break;
    }
    printf("â‚¹%d", amount);
    return 0;
}
