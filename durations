#include <stdio.h>
int main() {
     int duration;
    scanf("%d", &duration);
    switch(duration) {
        case 1:
         printf("₹1500");
          break;
        case 2:
         printf("₹4000");
          break;
        case 3:
         printf("₹7000");
          break;
    }
    return 0;
}
